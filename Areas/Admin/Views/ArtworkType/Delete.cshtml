@model Art.Models.ArtworkType
@{
    ViewData["Title"] = "Delete";
}
<br />
<h2 class="text-info">Artwork Type Delete</h2>
<br />
<form method="post">
    <div class="border backgroundWhite">
        <input type="hidden" asp-for="Id" />
        <fieldset disabled>
            <div class="form-group row">
                <div class="col-2">
                    <label asp-for="Name" class="custom-label"></label>
                </div>
                <div class="col-5">
                    <input asp-for="Name" class="form-control" />
                </div>
            </div>
            <div class="form-group row">
                <div class="col-2">
                    <label asp-for="Medium.Name" class="custom-label"></label>
                </div>
                <div class="col-5">
                    <input asp-for="Medium.Name" class="form-control" />
                </div>
            </div>
        </fieldset>
        <br />
        <div class="col-1"></div>
        
        <div class="form-group row">
            <div class="col-5 offset-2">
                <partial name="_DeleteAndBackToListButton" model="Model.Id" />
            </div>
        </div>
    </div>
</form>